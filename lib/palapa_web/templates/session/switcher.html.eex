<%= link("My profile", to: member_path(@conn, :show, @current_organization, @current_member)) %>
<%= link("Log out", to: session_path(@conn, :delete), class: "btn text-red",
  data: [controller: "link", action: "link#delete"]) %>

<ul class="list-reset">
  <span>Logged in as <%= @current_account.name %></span>
  <hr class="border">
  <%= for organization <- @organizations do %>
    <li><%= link organization.name, to: dashboard_path(@conn, :index, organization) %></li>
  <% end %>
</ul>
