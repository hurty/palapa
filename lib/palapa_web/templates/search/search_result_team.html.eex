<%= link to: Routes.member_path(@conn, :index, @current_organization, team_id: @search_result.team.id) do %>
<div class="flex px-2">
  <div class="w-8">
    <i class="fas fa-users text-gray-600"></i>
  </div>

  <div class="flex-1">
    <div class="text-green-500 font-bold text-sm"><%= Helpers.truncate_string(@search_result.team.name) %></div>
    <div>
      <span class="text-xs text-gray-600 font-bold"><%= gettext("Team") %></span>
    </div>
  </div>
</div>
<% end %>
