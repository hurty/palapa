<%= link to: member_path(@conn, :show, @conn.assigns.current_organization, @member), class: "h-full" do %>
  <div class="text-center"><%= PalapaWeb.MemberView.avatar(@member.account, :medium) %></div>
  <div class="text-center capitalize text-green"><%= @member.account.name %></div>
  <div class="text-grey-dark text-center text-xs uppercase"><%= @member.title %></div>
<% end %>
